.numBtn {
    margin: 10px;
    &:first-child {
        margin-left: 0;
    }
}

.loading {
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.3);
    background-image: url(./images/page-loading.gif);
    background-size: 50px;
    background-repeat: no-repeat;
    background-position: center;
    width: 100%;
    height: 100%;
    z-index: 11;
}
